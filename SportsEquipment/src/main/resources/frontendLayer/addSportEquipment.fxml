<AnchorPane prefHeight="552.0" prefWidth="509.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="frontendLayer.addSportEquipment">
