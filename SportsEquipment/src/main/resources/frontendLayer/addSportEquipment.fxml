            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Pane layoutX="11.0" layoutY="41.0" prefHeight="491.0" prefWidth="489.0" styleClass="mainPane" stylesheets="@application.css">
            <children>
                <Label layoutX="42.0" layoutY="55.0" text="Name">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Label>
                <Button layoutX="129.0" layoutY="165.0" mnemonicParsing="false" onMouseClicked="#add" prefHeight="39.0" prefWidth="231.0" styleClass="highlight" text="Add">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
                <Label layoutX="46.0" layoutY="159.0">
